	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Set default dates for current month",
					"const now = new Date();",
					"const startOfMonth = new Date(now.getFullYear(), now.getMonth(), 1);",
					"const endOfMonth = new Date(now.getFullYear(), now.getMonth() + 1, 0);",
					"",
					"pm.collectionVariables.set('start_date', startOfMonth.toISOString().split('T')[0]);",
					"pm.collectionVariables.set('end_date', endOfMonth.toISOString().split('T')[0]);",
					"",
					"// Set default sponsor query",
					"pm.collectionVariables.set('sponsor_query', 'coach');"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Global test script for all requests",
					"pm.test('Response time is less than 5000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(5000);",
					"});",
					"",
					"pm.test('Response has success field', function () {",
					"    const jsonData = pm.response.json();",
					"    pm.expect(jsonData).to.have.property('success');",
					"});",
					"",
					"// Store coach token from coach login responses",
					"if (pm.response.json().success && pm.response.json().token && pm.request.url.path.includes('auth/login') && !pm.request.url.path.includes('admin')) {",
					"    pm.collectionVariables.set('coach_token', pm.response.json().token);",
					"    console.log('Coach token stored:', pm.response.json().token);",
					"}",
					"",
					"// Store admin token from admin login responses",
					"if (pm.response.json().success && pm.response.json().token && pm.request.url.path.includes('admin/auth/login')) {",
					"    pm.collectionVariables.set('admin_token', pm.response.json().token);",
					"    console.log('Admin token stored:', pm.response.json().token);",
					"}"
				]
			}
		}
