					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.token) {",
									"        pm.collectionVariables.set('coach_token', response.token);",
									"        pm.collectionVariables.set('coach_id', response.user.id);",
									"        console.log('Coach token set:', response.token);",
									"    }",
									"}"
								]
							}
						}
					]
				}
			]
